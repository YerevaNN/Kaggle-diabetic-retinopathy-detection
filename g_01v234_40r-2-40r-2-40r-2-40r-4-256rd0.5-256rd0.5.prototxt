  dropout_param {
    dropout_ratio: 0.5
  }
  bottom: "ip1"
  top: "ip1"
}
layer {
  name: "ip1-1"
  type: "InnerProduct"
  bottom: "ip1"
  top: "ip1-1"
  param {
    lr_mult: 1
